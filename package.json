    "deploy:all": "npm run deploy && npm run deploy:admin",
    "cf-typegen": "wrangler types"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "typescript": "^5.8.3",
    "wrangler": "^4.13.0"
  }
}
